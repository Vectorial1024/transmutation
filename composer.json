    "type": "library",
    "keywords": [
        "library",
        "utility",
        "arrayaccess",
        "traversable",
        "arrayobject",
        "arrayiterator",
        "splobjectstorage",
        "splfixedarray",
        "weakmap"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Vectorial1024\\Transmutation\\": "src/"
        }
    },
    "autoload-dev": {
